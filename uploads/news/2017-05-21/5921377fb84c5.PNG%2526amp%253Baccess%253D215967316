    <script type="text/javascript">
    window.fSubmit = function (form) {
        // 当检索词是图片时，跳转到识图
        var keyword = form.word.value;
        var tiaoma = /^http[s]*\:\/\/[\s\S]*\.[jpg|gif|png|bmp|jpeg]*$/.test(keyword)
        || /img5\.imgtn\.bdimg\.com/.test(keyword)
        || /mt1\.baidu\.com\/timg/.test(keyword);
        var jumpURL = '/n/pc_search?queryImageUrl=' + encodeURIComponent(keyword)
        + '&fm=searchresult&pos=urlsearch&uptype=urlsearch';
        if (tiaoma) {
            location.href = jumpURL;
            return false;
        }
    };
    </script>
